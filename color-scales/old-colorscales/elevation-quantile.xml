<ColorMap name="elevation-quantile">
  <!-- An elevation theme color scale intended for small scale maps -->
  <Scaling>Quantile</Scaling>
  <ReliefShading>0</ReliefShading>
  <Interpolate>1</Interpolate>
  <!--  null values will be transparent  -->
  <NullColor color="0,0,0" opacity="0"/>
  <!--  Dark Green  -->
  <Color value="0.0" color="45,89,0"/>
  <!--  Light Green  -->
  <Color value="1.0" color="119,190,119"/>
  <!--  Yellow  -->
  <Color value="2.0" color="253,253,150"/>
  <!--  Orangish  -->
  <Color value="3.0" color="255,179,71"/>
  <!--  Brown  -->
  <Color value="4.0" color="130,105,83"/>
  <!--  White  -->
  <Color value="5.0" color="255,255,255"/>
</ColorMap>
